    package="fall2018.csc2017.gamehub">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Game Hub"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateUser" />
        <activity android:name=".GameHub" />
        <activity android:name=".ShowScoreboardChoice" />
        <activity android:name=".ShowScoreboard" />
        <activity android:name=".StartingActivity" />
        <activity android:name=".GameActivity" />
        <activity android:name=".SlidingTileEnd" />
        <activity android:name=".MatchingGame.StartingActivityMatch" />
        <activity android:name=".MatchingGame.GameActivityMatch" />
        <activity android:name=".MatchingGame.scoreboardChoiceMatch" />
        <activity android:name=".TowerOfHano.GameActivity" />
        <activity android:name=".TowerOfHano.StartingActivity" />
        <activity android:name=".TowerOfHano.HanoEnd" />
        <activity android:name=".TowerOfHano.ShowScoreboardChoiceHano" />
        <activity android:name=".TowerOfHano.ShowScoreboardHano" />
